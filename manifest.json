{
    "name": "iframeKiller",
    "version": "1.0",
    "description": "scans webpages for <iframe>",
    "permissions": [
        "storage"
    ],
    "background": {
        "scripts": [
                "background.js"
            ],
            "persistent": false
        },
    "page_action": {
            "default_popup": "popup.html"
        },
    "manifest_version": 2
}